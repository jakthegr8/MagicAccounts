<div class="group_header wellblue">
    <h3> Users </h3>
    <strong><%= link_to "Create User", new_user_path, :method => "get", :class => 'btn primary' %></strong>
</div>
<% if @users.empty? %>
    You don't own any accounts at this point. <br/><br/>
<%else%>
<table class="table table-striped">
 <thead>
  <tr>
    <th>ID</th>
        <th>
      <%= t("activerecord.attributes.user.name", :default => t("activerecord.labels.name", :default => "Name")) %>
    </th>
        <th>Created at</th>
    <th>Actions</th>
  </tr>
  </thead>
  <tbody>
  <% @users.each do |user| -%>
   <tr>
    <td><%= user.id %></td>
        <td><%= link_to user.name, user_path(user) %>
    </td>
        <td><%= user.created_at %></td>
    <td>      
      <%= link_to image_tag("edit_16x16.png"), edit_user_path(user) %>
      <%= link_to image_tag("delicon.png"), user_path(user), :method => :delete, :confirm => "#{t("web-app-theme.confirm", :default => "Are you sure?")}" %>
    </td>
    </tr>
    <% end end -%>
   </tbody>
</table>